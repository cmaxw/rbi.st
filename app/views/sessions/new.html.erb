<h1>New session</h1>

<%= form_tag login_path do %>
  <div class="field">
    <%= label_tag :username, "Username" %>
    <%= text_field_tag :username %>
  </div>

  <div class="field">
    <%= label_tag :password, "Password" %>
    <%= password_field_tag :password %>
  </div>

  <div class="buttons">
    <%= submit_tag "Log in" %>
  </div>
<% end %>
